# Server configuration
PORT=8080
ORIGIN=http://localhost:5173

# Google OAuth configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Session secret
SESSION_SECRET=your_session_secret

# SAP HANA Connection Details
HANA_HOST=your_hana_host
HANA_PORT=your_hana_port
HANA_URL=jdbc:sap://your_hana_host:your_hana_port?encrypt=true&validateCertificate=true
HANA_DRIVER=com.sap.db.jdbc.Driver

# UAA (User Account and Authentication) for SAP HANA Cloud
UAA_CLIENT_ID=your_uaa_client_id
UAA_CLIENT_SECRET=your_uaa_client_secret
UAA_URL=https://your_uaa_url
UAA_IDENTITY_ZONE=your_identity_zone
UAA_IDENTITY_ZONE_ID=your_identity_zone_id
UAA_TENANT_ID=your_tenant_id
UAA_VERIFICATION_KEY="-----BEGIN PUBLIC KEY-----\nYourPublicKeyHere\n-----END PUBLIC KEY-----"
UAA_DOMAIN=your_uaa_domain
UAA_BINDING_ID=your_binding_id
